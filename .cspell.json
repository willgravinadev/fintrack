{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "import": [
    "@cspell/dict-typescript",
    "@cspell/dict-software-terms",
    "@cspell/dict-node",
    "@cspell/dict-en_us",
    "@cspell/dict-en-gb",
    "@cspell/dict-npm",
    "@cspell/dict-html",
    "@cspell/dict-companies",
    "@cspell/dict-filetypes",
    "@cspell/dict-bash",
    "@cspell/dict-lorem-ipsum/cspell-ext.json",
    "@cspell/dict-es-es/cspell-ext.json"
  ],
  "dictionaries": [
    "software-terms",
    "npm",
    "fullstack",
    "redis",
    "typescript",
    "softwareTerms",
    "node",
    "en_us",
    "en-gb",
    "html",
    "companies",
    "misc",
    "filetypes",
    "bash",
    "lorem-ipsum",
    "es-es"
  ],
  "files": [
    "**",
    ".vscode/**",
    ".github/**"
  ],
  "ignorePaths": [
    "pnpm-lock.yaml",
    ".pnpm-store/",
    "packages/db/src/migrations/**"
  ],
  "ignoreRegExpList": [
    "apiKey='[a-zA-Z0-9-]{32}'"
  ],
  "useGitignore": true,
  "version": "0.2",
  "language": "en",
  "allowCompoundWords": true,
  "words": [
    "cmdk",
    "embla",
    "gravinawill",
    "hiett",
    "mdast",
    "Sakib",
    "shadcn",
    "shiki",
    "shikijs",
    "sonarjs",
    "sonner",
    "vaul"
  ]
}
